{
      "name": "joi-password",
      "version": "1.4.5",
      "description": "A Joi extension that help to validate a complex password",
      "main": "lib/index.js",
      "types": "lib/index.d.ts",
      "files": [
            "lib/**/*"
      ],
      "keywords": [
            "joi",
            "password",
            "validation",
            "validator"
      ],
      "scripts": {
            "build": "tsc",
            "prepare": "npm run build",
            "start:dev": "NODE_ENV=development  nodemon --exec ts-node index.ts",
            "test": "jest",
            "test:watch": "jest --watchAll --runInBand --detectOpenHandles",
            "test:cov": "jest --coverage --runInBand --detectOpenHandles"
      },
      "author": {
            "email": "heaty566@gmail.com",
            "name": "Pham Vinh Nhan",
            "url": "https://github.com/Heaty566"
      },
      "license": "ISC",
      "dependencies": {},
      "devDependencies": {
            "joi": "^17.4.0",
            "@babel/preset-env": "^7.12.17",
            "@types/jest": "^26.0.20",
            "@types/joi": "^17.2.3",
            "jest": "^26.6.3",
            "nodemon": "^2.0.7",
            "ts-jest": "^26.5.1",
            "ts-node": "^9.1.1",
            "typescript": "^4.1.5"
      },
      "directories": {
            "test": "test"
      },
      "repository": {
            "type": "git",
            "url": "git+https://github.com/Heaty566/joi-password.git"
      },
      "bugs": {
            "url": "https://github.com/Heaty566/joi-password/issues"
